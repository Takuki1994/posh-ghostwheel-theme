{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "valid_line": {
    "template": "\uf42e ",
    "foreground": "green",
    "background": "transparent"
  },
  "transient_prompt": {
    "template": " \uf101 ",
    "foreground": "#c0c0c0",
    "background": "transparent"
  },
  "error_line": {
    "template": "\ue009 ",
    "foreground": "red",
    "background": "transparent"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\uf0e7",
          "foreground": "yellow",
          "type": "root",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\uf460 ",
          "foreground": "#546E7A",
          "type": "root",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "<#546E7A>\u2026</>",
            "folder_separator_icon": "<#546E7A>/</>",
            "style": "folder"
          },
          "template": "{{ if .RootDir }}\ue271 {{ else if matchP \"^C:/Users/[^/]+$\" .Location }}\uf46d {{ else if matchP \"^\\\\.[^/]+$\" .Path }}\uf4c5 {{ else if glob \".git\\\\\\\\*\" }}<#f74e27>\ue65d</> {{ else if matchP \"^//[^/]+\" .Location }}\uf1e0 {{ else }}\uf115 {{ end }}",
          "foreground": "blue",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "<#546E7A>\u2026</>",
            "folder_separator_icon": "<#546E7A>/</>",
            "max_depth": 1,
            "style": "agnoster_short"
          },
          "template": "{{ .Path }}",
          "foreground": "cyan",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\uf460 ",
          "foreground": "#546E7A",
          "type": "git",
          "style": "plain"
        },
        {
          "properties": {
            "branch_ahead_icon": "\uf176",
            "branch_behind_icon": "\uf175",
            "branch_icon": "\ue725",
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "template": "{{ .UpstreamIcon }} ",
          "foreground": "default",
          "type": "git",
          "style": "plain",
          "foreground_templates": [
            "{{ if .Working.Changed }}yellow{{ end }}",
            "{{ if .Staging.Changed }}green{{end}}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}red{{ end }}",
            "{{ if gt .Ahead 0 }}magenta{{ end }}",
            "{{ if gt .Behind 0 }}blue{{ end }}"
          ]
        },
        {
          "properties": {
            "branch_ahead_icon": "\uf176",
            "branch_behind_icon": "\uf175",
            "branch_icon": "\ue725",
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "template": "{{ .HEAD }} ",
          "foreground": "default",
          "type": "git",
          "style": "plain",
          "foreground_templates": [
            "{{ if .Working.Changed }}yellow{{ end }}",
            "{{ if .Staging.Changed }}green{{end}}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}red{{ end }}",
            "{{ if gt .Ahead 0 }}magenta{{ end }}",
            "{{ if gt .Behind 0 }}blue{{ end }}"
          ],
          "min_width": 62
        },
        {
          "properties": {
            "branch_ahead_icon": "\uf176",
            "branch_behind_icon": "\uf175",
            "branch_icon": "\ue725",
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "template": "{{if .BranchStatus }}{{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf0c7 {{ .StashCount }}{{ end }}",
          "foreground": "default",
          "type": "git",
          "style": "plain",
          "foreground_templates": [
            "{{ if .Working.Changed }}yellow{{ end }}",
            "{{ if .Staging.Changed }}green{{end}}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}red{{ end }}",
            "{{ if gt .Ahead 0 }}magenta{{ end }}",
            "{{ if gt .Behind 0 }}blue{{ end }}"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "overflow": "hide",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none",
            "display_exit_code": true
          },
          "template": "{{ if .Error }}{{ .String }} \uf40c{{ else }}\uf00c{{ end }}",
          "foreground": "#00ff00",
          "type": "status",
          "style": "plain",
          "foreground_templates": [
            "{{ if .Error}}#e91e63{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \u2039",
          "foreground": "#546E7A",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " cmake {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue61e",
          "foreground": "#2cb25b",
          "type": "cmake",
          "style": "plain",
          "min_width": 62
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue61d ",
          "foreground": "#2cb25b",
          "type": "cmake",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u2039",
          "foreground": "#546E7A",
          "type": "cmake",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "context"
          },
          "template": " {{ if .Venv }}{{ .Venv }}{{ end }}",
          "foreground": "yellow",
          "type": "python",
          "style": "plain",
          "min_width": 62
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "context"
          },
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "yellow",
          "type": "python",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "context"
          },
          "template": " \ue73c ",
          "style": "plain",
          "foreground": "blue",
          "type": "python"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "context"
          },
          "template": "\u2039",
          "foreground": "#546E7A",
          "type": "python",
          "style": "plain"
        },
        {
          "template": "{{ if .Env.VCTOOLSVERSION }}VS Developer Mod \ue70c \u2039{{ end }}",
          "foreground": "#546E7A",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "mapped_shell_names": {
              "bash": "bash",
              "cmd": "Cmd",
              "pwsh": "Powershell"
            }
          },
          "template": " {{ .Name }}",
          "foreground": "default",
          "type": "shell",
          "style": "plain",
          "min_width": 62
        },
        {
          "properties": {
            "cache_duration": "none",
            "mapped_shell_names": {
              "bash": "\ue760",
              "cmd": "\uebc4",
              "pwsh": "\uebc7"
            }
          },
          "foreground": "default",
          "type": "shell",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u2039",
          "foreground": "#546E7A",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "charged_icon": "\ue22f ",
            "charging_icon": "<f>\ue234 </>",
            "discharging_icon": "<f>\ue231 </>"
          },
          "template": " {{ if not .Error }}{{ .Percentage }}\uf295 {{ .Icon }}{{ end }}",
          "foreground": "yellow",
          "type": "battery",
          "style": "plain",
          "foreground_templates": [
            "{{if eq \"Charging\" .State.String}}blue{{end}}",
            "{{if eq \"Discharging\" .State.String}}red{{end}}",
            "{{if eq \"Full\" .State.String}}green{{end}}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u2039",
          "foreground": "#546E7A",
          "type": "battery",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "15:04:05"
          },
          "foreground": "cyan",
          "template": " {{ .Format }} \ue383 ",
          "type": "time",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u2039",
          "foreground": "#546E7A",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " {{ if .WSL }}WSL at{{ end }}",
          "foreground": "blue",
          "type": "os",
          "style": "plain",
          "min_width": 62
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " {{.Icon}} ",
          "foreground": "blue",
          "type": "os",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \uf101 ",
          "foreground": "#c0c0c0",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
